CREATE TABLE shipment (
shipment_id INT AUTO_INCREMENT PRIMARY KEY,
order_id INT UNIQUE,
shipment_date DATE NOT NULL,
delivery_date DATE,
status VARCHAR(50) NOT NULL,
tracking_number VARCHAR(100),
FOREIGN KEY (order_id) REFERENCES orders (order_id)
);

